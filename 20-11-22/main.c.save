#include<assert.h>
#include<stdio.h>
int my_strlen(const char*a)
{
    assert(a!=NULL);//保证指针的有效性
    int count=0;
    for(;*a!='\0';a++)
    {
        count++;
    }
    return count;
}
int main()
{
    char arr[]="abcdfs";
    printf("%d\n",my_strlen(arr));
    return 0;
}
